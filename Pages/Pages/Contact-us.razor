@page "/contact-us"

            <div class="container-fluid">

                <!-- Page Header -->
                <div class="d-md-flex d-block align-items-center justify-content-between my-4 page-header-breadcrumb">
                    <h1 class="page-title fw-semibold fs-18 mb-0">Contate-nos</h1>
                    <div class="ms-md-1 ms-0">
                        <nav>
                            <ol class="breadcrumb mb-0">
                                <li class="breadcrumb-item"><a href="javascript:void(0);">Páginas</a></li>
                                <li class="breadcrumb-item active" aria-current="page">Contate-nos</li>
                            </ol>
                        </nav>
                    </div>
                </div>
                <!-- Page Header Close -->

                <!-- Start::row-1 -->
                <div class="row justify-content-center">
                    <div class="col-xxl-12 col-xl-12 col-lg-12 col-md-12 col-xm-12">
                        <div class="card custom-card overflow-hidden">
                            <div class="card-body p-0">
                                <div class="contact-page-banner">
                                    <div class="text-center">
                                        <h3 class="fw-semibold text-fixed-white">Contate-nos !</h3>
                                        <h6 class="text-fixed-white mb-4 px-sm-0 px-3">Tem alguma dúvida? Adoraríamos ouvir de você. </h6>
                                        <button class="btn btn-success btn-wave">Contato <i class="ri-phone-line ms-1 align-middle"></i></button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xxl-10 col-xl-10 col-lg-10 col-md-10 col-sm-10 col-12">
                        <div class="row">
                            <div class="col-xxl-6 col-xl-6 col-lg-12 col-md-12 col-sm-12">
                                <div class="card custom-card contactus-form overflow-hidden">
                                    <div class="card-header">
                                        <div class="card-title">
                                            Entre em contato !
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <div class="row gy-3">
                                            <div class="col-xl-6">
                                                <label for="contact-address-firstname" class="form-label">Primeiro Nome :</label>
                                                <input type="text" class="form-control form-control-light" id="contact-address-firstname" placeholder="Insira seu nome">
                                            </div>
                                            <div class="col-xl-6">
                                                <label for="contact-address-lastname" class="form-label">Sobrenome :</label>
                                                <input type="text" class="form-control form-control-light" id="contact-address-lastname" placeholder="Insira seu sobrenome">
                                            </div>
                                            <div class="col-xl-6">
                                                <label for="contact-address-phone" class="form-label">Telefone :</label>
                                                <input type="text" class="form-control form-control-light" id="contact-address-phone" placeholder="Telefone">
                                            </div>
                                            <div class="col-xl-6">
                                                <label for="contact-address-email" class="form-label">Email :</label>
                                                <input type="email" class="form-control form-control-light" id="contact-address-email" placeholder="Email">
                                            </div>
                                            <div class="col-xl-6">
                                                <label for="contact-address-address" class="form-label">Endereço :</label>
                                                <textarea class="form-control form-control-light" id="contact-address-address" rows="2"></textarea>
                                            </div>
                                            <div class="col-xl-6">
                                                <label for="contact-mail-message" class="form-label">Mensagem :</label>
                                                <textarea class="form-control form-control-light" id="contact-mail-message" rows="2"></textarea>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="card-footer">
                                        <div class="text-center">
                                            <button class="btn btn-primary-light btn-wave">Enviar mensagem</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xxl-6 col-xl-6 col-lg-12 col-md-12 col-sm-12">
                                <div class="card custom-card contactus-form overflow-hidden">
                                    <div class="card-body">
                                        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d56430.39986947676!2d-54.46063555!3d-27.8743423!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x94f935a34ec1d7c9%3A0xe7e3f8652b5d62bf!2sSanta%20Rosa%2C%20RS!5e0!3m2!1spt-BR!2sbr!4v1726536011141!5m2!1spt-BR!2sbr" height="370" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
                                    </div>    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--End::row-1 -->

            </div>
