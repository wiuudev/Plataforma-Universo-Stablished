                            <ApexPointSeries TItem="AreaIrregularchart" Items="AreaIrregularcharts" Name="Product A"  SeriesType="SeriesType.Area" XValue="@(e => e.IrregularDate)" YValue="@(e => e.ProductA)" />
                            <ApexPointSeries TItem="AreaIrregularchart" Items="AreaIrregularcharts" Name="Product B"  SeriesType="SeriesType.Area" XValue="@(e => e.IrregularDate)" YValue="@(e => e.ProductB)" />
                            <ApexPointSeries TItem="AreaIrregularchart" Items="AreaIrregularcharts" Name="Product C"  SeriesType="SeriesType.Area" XValue="@(e => e.IrregularDate)" YValue="@(e => e.ProductC)" />
                        </ApexChart>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-xl-6">
            <div class="card custom-card">
                <div class="card-header">
                    <div class="card-title">Area Chart With Null Values</div>
                </div>
                <div class="card-body">
