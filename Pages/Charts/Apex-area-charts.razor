@page "/apex-area-charts"
@using ApexCharts
@using System.Collections.Generic

<div class="container-fluid">
    <!-- Page Header -->
    <div class="d-md-flex d-block align-items-center justify-content-between my-4 page-header-breadcrumb">
        <h1 class="page-title fw-semibold fs-18 mb-0">Apex Area Charts</h1>
        <div class="ms-md-1 ms-0">
            <nav>
                <ol class="breadcrumb mb-0">
                    <li class="breadcrumb-item"><a href="javascript:void(0);">Apex Charts</a></li>
                    <li class="breadcrumb-item active" aria-current="page">Apex Area Charts</li>
                </ol>
            </nav>
        </div>
    </div>
    <!-- Page Header Close -->

    <!-- Start::row-1 -->   
    <div class="row">
        <div class="col-xl-6">  
            <div class="card custom-card">
                <div class="card-header">
                    <div class="card-title">Basic Area Chart</div>
                </div>
                <div class="card-body">
                    <ApexChart TItem="Areabasicchart" Height="320" Title="Fundamental Analysis of Stocks" Options=BasicAreaChartOptions  XAxisType="XAxisType.Datetime">
                        <ApexPointSeries TItem="Areabasicchart" Items="Areabasiccharts" Name="STOCK ABC"  SeriesType="SeriesType.Area" XValue="@(e => e.StockValue)" YAggregate="@(e => e.Sum(e => e.prices))" />
                    </ApexChart>
                </div>
            </div>
        </div>
        <div class="col-xl-6">
            <div class="card custom-card">
                <div class="card-header">
                    <div class="card-title">Spline Area Chart</div>
                </div>
                <div class="card-body">
                    <ApexChart TItem="AreaSplinechart" Height="320" Options=SplineAreaChartOptions  XAxisType="XAxisType.Datetime">
                        <ApexPointSeries TItem="AreaSplinechart" Items="AreaSplinecharts" Name="Series-1"  SeriesType="SeriesType.Area" XValue="@(e => e.SeriesValue)" YAggregate="@(e => e.Sum(e => e.Series1Value))" />
                        <ApexPointSeries TItem="AreaSplinechart" Items="AreaSplinecharts" Name="Series-2"  SeriesType="SeriesType.Area" XValue="@(e => e.SeriesValue)" YAggregate="@(e => e.Sum(e => e.Series2Value))" />
                    </ApexChart>
                </div>
            </div>
        </div>
        <div class="col-xl-6">
            <div class="card custom-card">
                <div class="card-header">
                    <div class="card-title">Area Chart With Negative Values</div>
                </div>
                <div class="card-body">
                    <ApexChart TItem="AreaNegativechart" Height="355" Title="Area with Negative Values" Options=NegativeAreaChartOptions>
                        <ApexPointSeries TItem="AreaNegativechart" Items="AreaNegativecharts" Name="North"  SeriesType="SeriesType.Area" XValue="@(e => e.Year)" YAggregate="@(e => e.Sum(e => e.North))" />
                        <ApexPointSeries TItem="AreaNegativechart" Items="AreaNegativecharts" Name="South"  SeriesType="SeriesType.Area" XValue="@(e => e.Year)" YAggregate="@(e => e.Sum(e => e.South))" />
                    </ApexChart>
                </div>
            </div>
        </div>
        <div class="col-xl-6">
            <div class="card custom-card">
                <div class="card-header">
                    <div class="card-title">Selection-Github Style Chart</div>
                </div>
                <div class="card-body">
                    <div id="chart-months">
                            <ApexChart TItem="TimeSeries" Options=BrushAreaChartOptions Height="140" XAxisType="XAxisType.Datetime">
                                <ApexPointSeries TItem="TimeSeries" Items="Brushdata" Name="Value" SeriesType="SeriesType.Area" XValue="@(e => e.Date)"
                                            YAggregate="@(e => e.Sum(e => e.Value))"  />
                            </ApexChart>
                    </div>
                    <div class="github-style d-flex align-items-center">
                        <div class="me-2">
                            <img class="userimg rounded" src="../assets/images/faces/1.jpg"
                                data-hovercard-user-id="634573" alt="" width="38" height="38">
                        </div>
                        <div class="userdetails lh-1">
                            <a class="username fw-semibold fs-14">coder</a>
                            <span class="cmeta d-block mt-1">
                                <span class="commits"></span> commits
                            </span>
                        </div>
                    </div>
                    <div id="chart-years">
                        <ApexChart TItem="TimeSeries" Options=Brush2AreaChartOptions Height="160" XAxisType="XAxisType.Datetime">
                            <ApexPointSeries TItem="TimeSeries" Items="Brushdata" Name="Quantity" SeriesType="SeriesType.Area" XValue="@(e => e.Date)"
                                        YValue="@(e => e.Quantity)" />
                        </ApexChart>
                </div>
                </div>
            </div>
        </div>
        <div class="col-xl-6">
            <div class="card custom-card">
                <div class="card-header">
                    <div class="card-title">Stacked Area Chart</div>
                </div>
                <div class="card-body">
                    <div id="area-stacked">
                        <ApexChart TItem="AreaStackedchart" Height="350" Options=StackedAreaChartOptions  XAxisType="XAxisType.Datetime">
                            <ApexPointSeries TItem="AreaStackedchart" Items="AreaStackedcharts" Name="South"  SeriesType="SeriesType.Area" XValue="@(e => e.StackedDate)" YAggregate="@(e => e.Sum(e => e.South))" />
                            <ApexPointSeries TItem="AreaStackedchart" Items="AreaStackedcharts" Name="North"  SeriesType="SeriesType.Area" XValue="@(e => e.StackedDate)" YAggregate="@(e => e.Sum(e => e.North))" />
                            <ApexPointSeries TItem="AreaStackedchart" Items="AreaStackedcharts" Name="Central"  SeriesType="SeriesType.Area" XValue="@(e => e.StackedDate)" YAggregate="@(e => e.Sum(e => e.Central))" />
                        </ApexChart>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-xl-6">
            <div class="card custom-card">
